  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "trainData2018 = pd.read_csv('Datacsv/trainData2018.csv')\n",
    "trainData2019 = pd.read_csv('Datacsv/trainData2019.csv')"
   ]
